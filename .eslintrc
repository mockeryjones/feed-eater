{
  "ecmaFeatures": {
    "blockBindings": true,
    "forOf": true,
    "jsx": true,
    "arrowFunctions": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "strict": 0,
    "eqeqeq": [2, "allow-null"],
    "semi": 2,
    "indent": [2, 2],
    "comma-spacing": [2, {"before": false, "after": true}],
    "arrow-parens": 1,
    "valid-jsdoc": 0,
    "no-unused-vars": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-empty": 2,
    "no-undef": 2,
    "no-undefined": 0,
    "quotes": [2, "single"]
  }
}
